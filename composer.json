{
    "name": "kozo/partial",
    "description": "Partial plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "template"],
    "homepage": "https://github.com/kozo/Partial",
    "license": "MIT",
    "authors": [{
        "name": "saku",
        "homepage": "https://github.com/kozo",
        "role": "Author"
    }],
    "require": {
        "php": ">=8.1.0",
        "cakephp/cakephp": "~5.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "*"
    },
    "extra": {
        "installer-name": "Partial"
    },
    "autoload": {
        "psr-4": {
            "Partial\\": "src"
        }
    },
    "scripts": {
        "phpcs": "phpcs --colors -p -s --standard=CakePHP src/",
        "phpcbf": "phpcbf --colors --parallel=16 -p src/"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
